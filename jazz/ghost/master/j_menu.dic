Select.checkmenu
{
	"\0\s[0]*What do you want to CHECK? \n\n\![*]\q[Jazz,checkjazz] \n\![*]\q[Jazz's inventory,checkinvent] \n\![*]\q[Yourself,checkuser]\n\n\![*]\q[Back to menu,BACKTOMENU] \![*]\q[I'm done,CANCEL]\e\e"
}

Select.checkuser
{
	totbooks = TOINT(totbooks)
	totbooks = funbookgiven + hardbookgiven + easybookgiven
	cubegiven = TOINT(cubegiven)
	rockgiven = TOINT(rockgiven)
	rocketgiven = TOINT(rocketgiven)
	plushgiven = TOINT(plushgiven)
	puzzlegiven = TOINT(puzzlegiven)
	figgiven = TOINT(figgiven)
	totgift = TOINT(totgift)
	totgift = totbooks + cubegiven + rockgiven + rocketgiven + plushgiven + puzzlegiven + figgiven
	
	
	if gotwhatscoming == 1
	{"\0\s[50]"}
	else 
	{"\0"}
	--
	if gotwhatscoming == 1
	{"* %(username)\w8 - 38 ATK\w8 5 DEF"}
	elseif badbad >= 100
	{"* %(username)\w8 - 28 ATK\w8 3 DEF"}
	elseif badbad >= 50 && badbad < 100
	{"* %(username)\w8 - 18 ATK\w8 2 DEF"}
	elseif badbad >= 20 && badbad < 50
	{"* %(username)\w8 - 8 ATK\w8 1 DEF"}
	elseif badbad >= 6 && badbad < 20
	{"* %(username)\w8 - 2 ATK\w8 1 DEF"}
	else 
	{"* %(username)\w8 - 1 ATK\w8 1 DEF"}
	--
	"\n\n"
	-- 
	if totappoints > 10
	{"\w8* Repeat offender.\n"}
	--
	if gotwhatscoming == 1
	{"\w8* Reached The End.\n"}
	elseif badbad >= 100
	{"\w8* Determined.\n"}
	elseif badbad >= 50 && badbad < 100
	{"\w8* Walking down a dangerous path.\n"}
	elseif badbad >= 20 && badbad < 50
	{"\w8* Not being the best %(heshe) can be.\n"}
	elseif badbad >= 6 && badbad < 20
	{"\w8* Made some mistakes.\n"}
	else 
	{"\w8* A human, \w4presumably.\n"}
	--
	if totgift > 0
	{"\w8* Has a lot of junk to give away.\n"}	
	--
	if fedjazz > 0
	{"\w8* Does not like people going hungry.\n"}
	--
	if petpet > 100
	{"\w8* Delivers a quantity of quality pets.\n"}
	--
	if beatbeat > 0
	{"\w8* Likes throwing punches.\n"}
	--
	if resetter > 0
	{"\w8* Does not believe in consequences.\n"}
	--
	if badbad < 10 
	{
	if skelelove == 1
	{"\w8* Reached the ultimate life goal: \w4charmed a skeleton.\n"}
	elseif lovelove >= 100
	{"\w8* Level 4 Skeleton Lover, \w4close to %(hisher) goal.\n"}
	elseif lovelove >= 50 && lovelove < 100
	{"\w8* Level 3 Skeleton Lover. \w8Spreading Skeleton Love around.\n"}
	elseif lovelove >= 20 && lovelove < 50
	{"\w8* Level 3 Skeleton Lover. \w8Raising that Skeleton Love meter.\n"}
	elseif lovelove >= 6 && lovelove < 20
	{"\w8* Level 2 Skeleton Lover. \w8Getting warmer.\n"}
	else 
	{"\w8* Not much of a skeleton lover.\n"}
	}
	else 
	{
		if jazzfriend == 1 
	{"\w8* Makes a rather bad friend, \w4all in all.\n"}
	if jazzlover == 1
	{"\w8* Makes a poor lover.\n"}
	else 
	{"\w8* Would probably be more popular if %(heshe) was not so bad.\n"}
	}
	--
	if nosey == 1 || seenplate == 1
	{"\w8* Likes sticking %(hisher) nose in someone else's business.\n"}
	--
	if truebackstory == 1
	{"\w8* ...Knows the story.\n"}
	--
	"\w8* That is all that can be said, \w4for now."
	--
	 "\w8\n\n\n\![*]\q[Back to menu,BACKTOMENU] \![*]\q[I'm done,CANCEL]\e"
}




