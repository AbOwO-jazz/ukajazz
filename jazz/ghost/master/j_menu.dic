//--OpenSakuraMenu

OpenSakuraMenu
{
		if gotwhatscoming == 1
		{
			"\0\s[50]\b2..."
			"\0\s[50]\b2*It is quiet."
			"\0\s[50]\b2*It is still there."
			"\0\s[50]\b2*Everything stays the same."
		}
		
	
			
		elseif totappoints > 30 || badbad > 10 
		{
			"\0\s[4]\b2%(jf)...DO YOU NEED SOMETHING?%(df)"
			"\0\s[4]\b2%(jf)...I'LL BE RIGHT AT IT.%(df)"
			"\0\s[4]\b2%(jf)OH,\w4 UM... CAN I HELP YOU?%(df)"
			"\0\s[4]\b2%(jf)OKAY?..%(df)"
		}
		
		
		elseif jazzlover == 1 || jazzfriend == 1 && badbad < 3
		{
			"\0\s[5]\b2%(jf)HI!!! CAN I HELP?%(df)"
			"\0\s[5]\b2%(jf)I'LL BE RIGHT AT IT!!!%(df)"
			"\0\s[5]\b2%(jf)I'LL BE HAPPY TO HELP YOU!!!%(df)"
			"\0\s[5]\b2%(jf)I'LL DO MY BEST FOR YOU!!!%(df)"
			
		}
		
		   else 
		   {
		"\0\s[0]\b2%(jf)DID YOU WANT SOMETHING?%(df)"
		"\0\s[0]\b2%(jf)JUST TELL ME IF YOU NEED ANY HELP!%(df)"
		"\0\s[0]\b2%(jf)DON'T HESITATE TO ASK ME IF YOU NEED ANYTHING!%(df)"
		"\0\s[0]\b2%(jf)I CAN DO MANY THINGS! PROMISE!%(df)"
		"\0\s[0]\b2%(jf)ASK, AND YOU SHALL RECEIVE!%(df)"
		"\0\s[0]\b2%(jf)HELLO! WHAT DO YOU WANT?%(df)"
		
		   }
		--
		MenuRun
}

//--MenuRun

MenuRun
 {	
 	wd = GetWeekDay
 	ap = GetAP
 	nowmin = Getnowmin
 	nowhour = Getnowhour
	--
	"\n\n\_q\b2" 
	--
	if lastTalk != "" 
	{
	  "\![*]\q[Could you say that again?,OnLastTalk]\n\n"
	
	}
	--

	
	"\![*]\q[Tell me something new,altalk]\n\n"
	--
   //"\![*]\q[I want to try,testvariable]\n/
	"\![*]\q[I want to CHECK,checkmenu]\n/
	\![*]\q[I want to talk,talk]\n/
	\![*]\q[I want to give you something,gift]\n/
	\![*]\q[I need some help,dosomething]\n\n/
	\![*]\q[Show me the Config Menu,configmenu]\n\n\n/
 	\![*]\q[Nevermind,CANCEL]\n\n%(wd) %(nowhour):%(nowmin) %(ap), %(month)/%(day)/%(year)\e"

	
}

Select.altalk
{
	AiTalk
}

OnLastTalk 
{
"%(lastTalk)\e"
}





